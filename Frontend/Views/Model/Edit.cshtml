@{
    if (ViewBag.ID == 0)
    {
        ViewBag.Title = "Create New Model";
    }
    else
    {
        ViewBag.Title = "Edit Model";
    }

    ViewBag.Module = "Model";
    ViewBag.CurrentSeason = Frontend.Helper.Common.GetCurrentSeason();
    //Frontend.Helper.AuthHelper.GetActionButtonStatus(ViewBag.ModuleCode, Frontend.Helper.ActionEnum.Create, HttpContext.Current))
    int canCreate = 0;
    if (Frontend.Helper.AuthHelper.GetActionButtonStatus(ViewBag.ModuleCode, Frontend.Helper.ActionEnum.Create, HttpContext.Current) != "disabled")
    {
        canCreate = 1;
    }
    string userManualUrl = string.Empty;
    if (System.IO.File.Exists(Server.MapPath("~/UserManual/" + ViewBag.ModuleCode + ".pdf")))
    {
        userManualUrl = "/UserManual/" + ViewBag.ModuleCode + ".pdf";
    }
}

<style>
    .tilsoft-table .tilsoft-table-body {
        max-height: none;
    }
</style>

<form class="row" name="editForm" id="main-form">
    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="jarviswidget" id="wid-id-general" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
            <header>
                <ul class="nav nav-tabs pull-left in">
                    <li class="active">
                        <a data-toggle="tab" href="#tabGeneral" name="gtab"> <span> General </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabSparepart"> <span> Sparepart </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabSubMaterial"> <span> Sub Material </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabPackagingMethod"> <span> Packaging </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabCushion"> <span> Cushion </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabFreescanImage"> <span> Freescan </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabGardenImage"> <span> Garden </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tab360Video"> <span> 360 </span> </a>
                    </li>

                    <li>
                        <a data-toggle="tab" href="#tabConfigPrice"> <span> Config Price </span> </a>
                    </li>
                </ul>
            </header>

            <div>
                <div class="widget-body">
                    <div class="tab-content">
                        <div class="tab-pane active" id="tabGeneral">
                            <div class="smart-form">
                                <fieldset>
                                    <div class="row">
                                        <section class="col col-2">
                                            <label class="label">Freescan Image</label>
                                            <div class="input-group">
                                                @Html.Partial("~/Views/Shared/_Controls/_ImageBox.cshtml", new Frontend.Models.ImageBoxViewModel() { URLBinding = "data.imageFile_DisplayUrl", JSOnChangeHandler = "event.changeImage", JSONRemoveHandler = "event.removeImage" })
                                            </div>
                                        </section>
                                        <section class="col col-10">
                                            <div class="row">
                                                <section class="col col-2">
                                                    <label class="label">Code</label>
                                                    <label class="input">
                                                        <span class="form-control">{{data.modelUD}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-8">
                                                    <label class="label">Name</label>
                                                    <label class="input" ng-if="!data.totalFactoryOrderItem && canCreate">
                                                        <input type="text" ng-model="data.modelNM" required />
                                                    </label>
                                                    <label class="input" ng-if="data.totalFactoryOrderItem && canCreate">
                                                        <input type="text" ng-model="data.modelNM" readonly />
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.modelNM}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">EAN Code</label>
                                                    <label class="input">
                                                        <span class="form-control">{{data.eanCode}}</span>
                                                    </label>
                                                </section>
                                            </div>

                                            <div class="row">
                                                <section class="col col-2">
                                                    <label class="label">Season</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.season" ng-options="season.seasonValue as season.seasonText for season in seasons">
                                                            <option value=""></option>
                                                        </select>
                                                        <i></i>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.season}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Type</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.productTypeID" ng-options="productType.productTypeID as productType.productTypeNM for productType in productTypes" required>
                                                            <option value=""></option>
                                                        </select>
                                                        <i></i>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.productTypeNM}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Status</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.modelStatusID" ng-options="modelStatus.modelStatusID as modelStatus.modelStatusNM for modelStatus in modelStatuses">
                                                            <option value=""></option>
                                                        </select>
                                                        <i></i>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.modelStatusNM}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Country</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.manufacturerCountryID" ng-options="manufacturerCountry.manufacturerCountryID as manufacturerCountry.manufacturerCountryNM for manufacturerCountry in manufacturerCountries" name="manufacturerCountryID" required>
                                                            <option value=""></option>
                                                        </select>
                                                        <span class="help-block" ng-show="editForm.manufacturerCountryID.$error.required"><i class="fa fa-warning"></i>Country is required.</span>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.manufacturerCountryNM}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Range Name</label>
                                                    <label class="input" ng-if="canCreate">
                                                        <input type="text" ng-model="data.rangeName" />
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.rangeName}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Collection</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.collection" ng-options="collect for collect in collections">
                                                            <option value=""></option>
                                                        </select>
                                                        <i></i>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.collection}}</span>
                                                    </label>
                                                </section>
                                            </div>

                                            <div class="row">
                                                <section class="col col-2">
                                                    <label class="label">Group</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.productGroupID" ng-options="productGroup.productGroupID as productGroup.productGroupNM for productGroup in productGroups" required>
                                                            <option value=""></option>
                                                        </select>
                                                        <i></i>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.productGroupNM}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Category</label>
                                                    <label class="select" ng-if="canCreate">
                                                        <select ng-model="data.productCategoryID" ng-options="category.productCategoryID as category.productCategoryNM for category in productCategories">
                                                            <option value=""></option>
                                                        </select>
                                                        <i></i>
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control">{{data.productCategoryNM}}</span>
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">Catalogue Page</label>
                                                    <label class="input" ng-if="canCreate">
                                                        <input type="text" style="text-align: right;" ng-model="data.cataloguePageNo" />
                                                    </label>
                                                    <label class="input" ng-if="!canCreate">
                                                        <span class="form-control" style="text-align: right;">{{data.cataloguePageNo}}</span>
                                                    </label>
                                                </section>



                                                <section class="col col-2">
                                                    <label class="label">Net Weight</label>
                                                    <label class="input">
                                                        <input type="text" ng-model="data.netWeight" />
                                                    </label>
                                                </section>

                                                <section class="col col-2">
                                                    <label class="label">HS Code</label>
                                                    <label class="input">
                                                        <input type="text" id="hsCode" name="hsCode" ng-model="data.hsCode" maxlength="20" ng-maxlength="8" />
                                                    </label>
                                                </section>
                                            </div>

                                            <div class="row">
                                                <section class="col col-3">
                                                    <label class="label">&nbsp;</label>
                                                    <label class="checkbox">
                                                        <input type="checkbox" ng-model="data.hasCushion">
                                                        <i></i>Cushion Enabled
                                                    </label>
                                                </section>
                                                <section class="col col-3">
                                                    <label class="label">&nbsp;</label>
                                                    <label class="checkbox">
                                                        <input type="checkbox" ng-model="data.hasFrameMaterial">
                                                        <i></i>Frame Enabled
                                                    </label>
                                                </section>
                                                <section class="col col-3">
                                                    <label class="label">&nbsp;</label>
                                                    <label class="checkbox">
                                                        <input type="checkbox" ng-model="data.hasSubMaterial">
                                                        <i></i>Sub Material Enabled
                                                    </label>
                                                </section>
                                                <section class="col col-3">
                                                    <label class="label">&nbsp;</label>
                                                    <label class="checkbox">
                                                        <input type="checkbox" ng-model="data.isExcludedInNotification">
                                                        <i></i> Excluded In Notification
                                                    </label>
                                                </section>
                                            </div>
                                        </section>
                                    </div>

                                    <div class="row">
                                        <section class="col col-8">
                                            <div class="tilsoft-table-wrapper">
                                                <div class="tilsoft-table" style="border-bottom: none;">
                                                    <div class="tilsoft-table-body" style="width: 100%; overflow-y: hidden; min-height: 0px; height: auto; margin-top: 0px;">
                                                        <table>
                                                            <tr>
                                                                <th colspan="4">DIMENSION</th>
                                                            </tr>
                                                            <tr>
                                                                <td>&nbsp;</td>
                                                                <td style="text-align: center;"><strong>L/D</strong></td>
                                                                <td style="text-align: center;"><strong>W</strong></td>
                                                                <td style="text-align: center;"><strong>H</strong></td>
                                                            </tr>
                                                            <tr>
                                                                <td>OVERALL</td>
                                                                <td><input type="text" ng-model="data.overallDimL" style="text-align: right;" /></td>
                                                                <td><input type="text" ng-model="data.overallDimW" style="text-align: right;" /></td>
                                                                <td><input type="text" ng-model="data.overallDimH" style="text-align: right;" /></td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                        <section class="col col-4">
                                            <div class="tilsoft-table-wrapper">
                                                <div class="tilsoft-table" style="border-bottom: none;">
                                                    <div class="tilsoft-table-body" style="width: 100%; overflow-y: hidden; min-height: 0px; height: auto; margin-top: 0px;">
                                                        <table>
                                                            <tr>
                                                                <th colspan="4">Factory</th>
                                                            </tr>
                                                            <tr>
                                                                <td style="text-align: center; width: 80px;">&nbsp;</td>
                                                                <td style="text-align: center; width: 60px; font-weight: bold;">Default</td>
                                                                <td style="text-align: center; font-weight: bold;">Code</td>
                                                            </tr>
                                                            <tr>
                                                                <td style="text-align: center;"><a href="javascript:void(0)" ng-click="event.add(data.defaultFactoryID, $event)" class="btn btn-primary btn-xs" style="width: 70px;">Add</a></td>
                                                                <td colspan="2">
                                                                    <select style="padding: 3px;" ng-model="data.defaultFactoryID" ng-options="factory.id as factory.label for factory in factoryDTOs">
                                                                        <option value=""></option>
                                                                    </select>
                                                                </td>
                                                            </tr>
                                                            <tr ng-repeat="item in data.modelDefaultFactories | orderBy: 'isDefault':true">
                                                                <td style="text-align: center;"><a href="javascript:void(0)" ng-click="event.removed($event, $index)" class="btn btn-danger btn-xs" style="width: 70px;">Remove</a></td>
                                                                <td style="text-align: center;">
                                                                    <label>
                                                                        <input type="radio" name="rdoDefaultFactory" ng-model="$parent.defaultFactoryID" ng-value="item.factoryID" />
                                                                    </label>
                                                                </td>
                                                                <td>{{item.factoryUD}}</td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                    </div>

                                    <section>
                                        <legend style="margin-top: 0px; padding-top: 0px; margin-bottom: 15px;">Pieces</legend>
                                        <div class="smart-form">
                                            <fieldset style="padding: 0px;">
                                                <section>
                                                    <label class="input">
                                                        <input type="text" placeholder="Provide your search criterial and press enter" id="quicksearchBoxPiece" ng-keypress="quicksearchPiece.event.searchBoxKeyPress($event)" autocomplete="off" />
                                                        <i class="icon-append fa fa-search"></i>
                                                    </label>
                                                    <div id="piece-popup" class="popup-container-2">
                                                        <div style="float: right; margin-bottom: 8px;">
                                                            <a ng-click="quicksearchPiece.event.ok($event)" class="btn btn-primary btn-xs font-11" href="javascript:void(0);"><i class="fa fa-plus"></i> Add selected items</a>
                                                            <a ng-click="quicksearchPiece.event.close($event)" class="btn btn-primary btn-xs font-11" href="javascript:void(0);"><i class="fa fa-remove"></i> Close</a>
                                                        </div>
                                                        <div class="clear"></div>
                                                        <div id="grdPieceSearchResult" class="tilsoft-table-wrapper">
                                                            <div class="tilsoft-table">
                                                                <div class="tilsoft-table-header" style="width: 760px;">
                                                                    <div style="width: 60px;"><input type="checkbox" ng-change="quicksearchPiece.event.selectAll()" ng-model="quicksearchPiece.selectValue" /></div>
                                                                    <div style="width: 100px;">Code</div>
                                                                    <div style="width: 100px;">EAN</div>
                                                                    <div>Name</div>
                                                                </div>
                                                                <div class="tilsoft-table-body" style="width: 760px;">
                                                                    <table>
                                                                        <tr ng-repeat="item in quicksearchPiece.data | orderBy:'+modelNM'">
                                                                            <td style="width: 60px; text-align: center;">
                                                                                <input type="checkbox" ng-model="item.isSelected" />
                                                                            </td>
                                                                            <td style="width: 100px;">{{item.modelUD}}</td>
                                                                            <td style="width: 100px;">{{item.eanCode}}</td>
                                                                            <td>{{item.modelNM}}</td>
                                                                        </tr>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </section>
                                            </fieldset>
                                            <fieldset style="padding: 0px; border-top: none;">
                                                <div id="grdPiece" class="tilsoft-table-wrapper">
                                                    <div class="tilsoft-table">
                                                        <div class="tilsoft-table-body" style="width: 100%; overflow-y: auto; height: auto; min-height: 100px; margin-top: 0px;">
                                                            <table>
                                                                <tr>
                                                                    <th style="width: 80px;"></th>
                                                                    <th>Code</th>
                                                                    <th style="width:45px;">Row Index</th>
                                                                    <th>Name</th>
                                                                    <th>Type</th>
                                                                    <th style="width:70px">Quantity</th>
                                                                    <th colspan="3">DIMENSION</th>
                                                                </tr>
                                                                <tr style="background-color:#eeefee">
                                                                    <td style="text-align: center;width: 80px;"></td>
                                                                    <td style=""></td>
                                                                    <td style=""></td>
                                                                    <td style=""></td>
                                                                    <td style=""></td>
                                                                    <td style="width:70px"></td>
                                                                    <td style="width:70px; text-align:center">L/D</td>
                                                                    <td style="width:70px; text-align:center">W</td>
                                                                    <td style="width:70px; text-align:center">H</td>
                                                                </tr>
                                                                <tr ng-repeat="item in data.pieces">
                                                                    <td style="text-align: center;width: 80px;">
                                                                        <a class="btn btn-danger btn-xs font-11" href="javascript:void(0);" ng-click="event.removePiece($event, item.modelPieceID)"><i class="fa fa-times-circle"></i> Remove</a>
                                                                    </td>

                                                                    <td style="">{{item.modelUD}}</td>
                                                                    <td><input type="text" ng-model="item.rowIndex" style="width: 100%;" /></td>
                                                                    <td style="">{{item.modelNM}}</td>
                                                                    <td style="">{{item.productTypeNM}}</td>
                                                                    <td style="width:70px"><input type="text" ng-model="item.quantity" style="width: 100%;" /></td>
                                                                    <td style="width:70px; text-align:center">{{item.overallDimL}}</td>
                                                                    <td style="width:70px; text-align:center">{{item.overallDimW}}</td>
                                                                    <td style="width:70px; text-align:center">{{item.overallDimH}}</td>
                                                                </tr>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </div>
                                    </section>

                                    <section>
                                        <div class="note">
                                            Create By
                                            <a ng-show="(data.createdBy != null && data.creatorName2 != null)" href="@Url.Action("Detail", "EmployeeMng", new { id = UrlParameter.Optional })/{{data.createdBy}}" data-featherlight="iframe" data-featherlight-iframe-allowfullscreen="true" data-featherlight-iframe-width="800" data-featherlight-iframe-height="600">
                                                {{data.creatorName2}}
                                            </a>
                                            <span ng-show="(data.createdBy != null && data.creatorName2 == null && data.creatorName2 != null)"> <i class="fa fa-user"></i> {{data.creatorName2}}</span>
                                            on {{data.createdDate}}
                                        </div>
                                        <div class="note">
                                            Updated By
                                            <a ng-show="(data.updatedBy != null && data.updatorName2 != null)" href="@Url.Action("Detail", "EmployeeMng", new { id = UrlParameter.Optional })/{{data.updatedBy}}" data-featherlight="iframe" data-featherlight-iframe-allowfullscreen="true" data-featherlight-iframe-width="800" data-featherlight-iframe-height="600">
                                                {{data.updatorName2}}
                                            </a>
                                            <span ng-show="(data.updatedBy != null && data.updatorName2 == null && data.updatorName != null)"> <i class="fa fa-user"></i> {{data.updatorName}}</span>
                                            on {{data.updatedDate}}
                                        </div>
                                    </section>
                                </fieldset>
                            </div>
                        </div>

                        <div class="tab-pane" id="tabSparepart">
                            <div class="smart-form">
                                <fieldset style="padding: 0px; border-top: none;">
                                    <a class="btn btn-primary btn-xs font-11" href="javascript:void(0);" ng-click="event.addPart()" style="margin-bottom: 10px;"><i class="fa fa-plus"></i> Add New </a>
                                    <div avs-scroll grid-handler="nullHandler" id="grdSparepart" class="tilsoft-table-wrapper">
                                        <div class="tilsoft-table">
                                            <div class="tilsoft-table-body" style="width: 100%; height: auto; min-height: 100px; margin-top: 0px; overflow-y:auto">
                                                <table>
                                                    <tr>
                                                        <th style="text-align: center;width: 120px;"></th>
                                                        <th style="width: 60px;">Code</th>
                                                        <th>Name</th>
                                                    </tr>
                                                    <tr ng-repeat="item in data.spareparts">
                                                        <td style="text-align: center;width: 120px;">
                                                            <a class="btn btn-danger btn-xs font-11" href="javascript:void(0);" ng-click="event.removePart($event, item.modelSparepartID)"><i class="fa fa-times-circle"></i> Remove</a>
                                                        </td>
                                                        <td style="text-align: center; width: 60px;">{{item.partUD}}</td>
                                                        <td><input type="text" ng-model="item.description" style="width: 100%;" /></td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                        <div class="tab-pane" id="tabSubMaterial">
                            <div class="smart-form">
                                <fieldset style="padding: 0px;">
                                    <section>
                                        <label class="input">
                                            <input type="text" placeholder="Provide your search criterial and press enter" id="quicksearchBoxSubMaterial" ng-keypress="quicksearchSubMaterial.event.searchBoxKeyPress($event)" autocomplete="off" />
                                            <i class="icon-append fa fa-search"></i>
                                        </label>
                                        <div id="SubMaterial-popup" class="popup-container-2">
                                            <div style="float: right; margin-bottom: 8px;">
                                                <a ng-click="quicksearchSubMaterial.event.ok($event)" class="btn btn-primary btn-xs font-11" href="javascript:void(0);"><i class="fa fa-plus"></i> Add selected items</a>
                                                <a ng-click="quicksearchSubMaterial.event.close($event)" class="btn btn-primary btn-xs font-11" href="javascript:void(0);"><i class="fa fa-remove"></i> Close</a>
                                            </div>
                                            <div class="clear"></div>
                                            <div id="grdSubMaterialSearchResult" class="tilsoft-table-wrapper">
                                                <div class="tilsoft-table">
                                                    <div class="tilsoft-table-header" style="width: 860px;">
                                                        <div style="width: 40px; padding-right: 8px;"><input type="checkbox" ng-model="quicksearchSubMaterial.selectValue" ng-change="quicksearchSubMaterial.event.selectAll()" /></div>
                                                        <div style="width: 80px;">Standard</div>
                                                        <div style="width: 80px;">Season</div>
                                                        <div style="width: 100px;">Image</div>
                                                        <div style="width: 100px;">Code</div>
                                                        <div>Name</div>
                                                    </div>
                                                    <div class="tilsoft-table-body" style="width: 860px;">
                                                        <table>
                                                            <tr ng-repeat="item in quicksearchSubMaterial.data | orderBy:'+subMaterialOptionNM'">
                                                                <td style="width: 40px; text-align: center;">
                                                                    <input type="checkbox" ng-model="item.isSelected" />
                                                                </td>
                                                                <td style="width: 80px; text-align: center;"><input type="checkbox" disabled ng-model="item.isStandard" /></td>
                                                                <td style="width: 80px; text-align: center;">{{item.season}}</td>
                                                                <td style="width: 100px; text-align: center; vertical-align: middle;"><img style="width: 95%;" ng-src="{{item.imageFile_DisplayUrl}}" /></td>
                                                                <td style="width: 100px;">{{item.subMaterialOptionUD}}</td>
                                                                <td>{{item.subMaterialOptionNM}}</td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </fieldset>
                                <fieldset style="padding: 0px; border-top: none;">
                                    <div id="grdSubMaterial" class="tilsoft-table-wrapper">
                                        <div class="tilsoft-table">
                                            <div class="tilsoft-table-body" style="width: 100%; overflow-y: auto; height: auto; min-height: 100px; margin-top: 0px;">
                                                <table>
                                                    <tr>
                                                        <th></th>
                                                        <th>Enabled</th>
                                                        <th>Confirmed</th>
                                                        <th>Standard</th>
