                                                <th style="font-weight: bold;width: 100px;background-color:rgba(7, 167, 12, 0.69)">Shipped in Cont  %</th>
                                                <th style="font-weight: bold;width: 100px;background-color:rgba(7, 167, 12, 0.69)">To be shipped in Cont</th>
                                                <th style="font-weight: bold;background-color:rgba(7, 167, 12, 0.69)">To be shipped in Cont %</th>
                                            </tr>
                                            <tr style="background-color:#cee9ff">
                                                <td colspan="2"><b>Total</b></td>
                                                <td style="font-weight:700;text-align:right">{{method.getTotal(data.supplierData,'orderQnt40HC') | number : 2}}</td>
                                                <td style="font-weight:700;text-align:right">${{method.getTotal(data.supplierData,'fobCostUSD') | number : 2}}</td>
                                                <td style="font-weight:700;text-align:right">${{method.getTotal(data.supplierData,'grossMarginAfterCommissionUSD') | number : 2}}</td>
                                                <td style="font-weight:700;text-align:right">{{method.getPercentage(method.getTotal(data.supplierData,'grossMarginAfterCommissionUSD'), method.getTotal(data.supplierData,'fobCostUSD')) | number : 2}}%</td>
                                                <td style="font-weight:700;text-align:right">{{method.getTotal(data.supplierData,'shippedQnt40HC') | number : 2}}</td>
                                                <td style="font-weight:700;text-align:right">{{method.getPercentage(method.getTotal(data.supplierData,'shippedQnt40HC'), method.getTotal(data.supplierData,'orderQnt40HC')) | number : 2}}%</td>
                                                <td style="font-weight:700;text-align:right">{{method.getTotal(data.supplierData,'toBeShippedQnt40HC') | number : 2}}</td>
                                                <td style="font-weight:700;text-align:right">{{method.getPercentage(method.getTotal(data.supplierData,'toBeShippedQnt40HC'), method.getTotal(data.supplierData,'orderQnt40HC')) | number : 2}}%</td>
                                            </tr>
                                            <tr ng-repeat="item in data.supplierData | orderBy:'-fobCostUSD'">
                                                <td style="text-align:left;">{{item.factoryUD}}</td>
                                                <td style="text-align:left;">{{item.factoryName}}</td>
                                                <td style="text-align:right">{{item.orderQnt40HC | number : 2}}</td>
                                                <td style="text-align:right;">${{item.fobCostUSD | currency:'':2}}</td>
                                                <td style="text-align:right;">${{item.grossMarginAfterCommissionUSD | currency:'':2}}</td>
                                                <td style="text-align:right;">{{method.getPercentage(item.grossMarginAfterCommissionUSD,item.fobCostUSD) | number : 2}}%</td>
                                                <td style="text-align:right">{{item.shippedQnt40HC | number : 2}}</td>
                                                <td style="text-align:right;">{{method.getPercentage(item.shippedQnt40HC,item.orderQnt40HC) | number : 2}}%</td>
                                                <td style="text-align:right">{{item.toBeShippedQnt40HC | number : 2}}</td>
                                                <td style="text-align:right;">{{method.getPercentage(item.toBeShippedQnt40HC,item.orderQnt40HC) | number : 2}}%</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </article>
</div>

