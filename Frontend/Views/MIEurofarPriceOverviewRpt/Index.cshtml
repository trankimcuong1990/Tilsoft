@{
    ViewBag.Title = "Eurofar Price Overview";
    ViewBag.Module = "Eurofar Price Overview";
}
<style>
    .jarviswidget.well header{display:block;}
    .tilsoft-table .tilsoft-table-body table .sale-table th {
        background: none !important;
    }
    .noHover {
        pointer-events: none;
    }
    .sale-table tr:nth-child(1) {
        background-color: #ffe2a6;
    }
    .sale-table tr:nth-child(2) {
        background-color: #7ebfff;
    }
    .sale-table tr:nth-child(3) {
        background-color: #beffa9;
    }
    .sale-table tr:nth-child(4) {
        background-color: #f1a5ff;
    }
    .sale-table tr:nth-child(5) {
        background-color: #a5e2ff;
    }
    .sale-table tr:nth-child(6) {
        background-color: #ffa2c0;
    }
    .sale-table tr:nth-child(7) {
        background-color: #bfb8ff;
    }
    .sale-table tr:nth-child(8) {
        background-color: #4dfb9a;
    }
</style>
<div class="row">
    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="jarviswidget jarviswidget-color-darken" id="wid-id-search-filter" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
            <header>
                <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                <h2>Filters</h2>
            </header>

            <div>
                <div class="widget-body no-padding">
                    <div class="smart-form">
                        <fieldset>
                            <section class="col col-4">
                                <label class="label">Season</label>
                                <label class="select">
                                    <select class="form-control" ng-model="selection.season" ng-options="season.seasonValue as season.seasonText for season in seasons"></select>
                                    <i></i>
                                </label>
                            </section>
                        </fieldset>
                        <footer>
                            <a href="javascript:void(0)" class="btn btn-primary" ng-click="event.generateHtml($event)" style="float: none;"><i class="fa fa-bar-chart-o"></i> Html Version</a>
                            <a href="javascript:void(0)" class="btn btn-primary" ng-click="event.generateExcel($event)" style="float: none;"><i class="fa fa-file-excel-o"></i> Excel Version</a>
                        </footer>
                    </div>
                </div>
            </div>
        </div>
    </article>
</div>
<div ng-if="show" class="row" style="margin-top:15px;">
    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="jarviswidget well jarviswidget-color-darken" id="wid-id-0" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
            <header>
                <span class="widget-icon"><i class="fa fa-table"></i></span>
                <h2>Margin Conclusion</h2>
            </header>
            <div>
                <div class="widget-body no-padding">
                    <fieldset style="padding-top:0px;">
                        <div class="row">
                            <div class="tilsoft-table">
                                <div class="tilsoft-table-body" style="margin-top: 0px">
                                    <table>
                                        <tbody>
                                            <tr style="padding-right:1em">
                                                <th style="background-color:#FF7599">Sale Manager</th>
                                                <th style="width:80px">Order<br /> in Cont<br /> with <br />cost<br /> Price</th>
                                                <th style="width:100px">FOB Cost<br /> Price <br />in USD</th>
                                                <th style="background-color:#FF7599; width:100px">FOB Sales<br /> amount<br /> in USD</th>
                                                <th style="background-color:#FF7599; width:70px">% FOB<br /> Sales</th>
                                                <th style="width:100px">DELTA<br /> IN USD</th>
                                                <th style="width:70px">%<br /> DELTA</th>
                                                <th style="background-color:#FF7599; width:100px">Delta after<br /> All</th>
                                                <th style="background-color:#ebf1de; width:70px">% Delta after<br /> All</th>
                                                <th style="background-color:darkorange; width:70px">% OF<br /> 100% <br />DELTA<br />AFTER<br /> All</th>
                                                <th style="width:80px">Commission<br /> in USD</th>
                                                <th style="width:80px">Damages<br /> (1,25% <br />FOB Cost)</th>
                                                <th style="width:80px">LC Cost</th>
                                                <th style="width:80px">Interest<br /> Cost &nbsp;</th>

                                                <th style="width:80px">Trans port Cost</th>
                                                <th style="width:80px">Inspec tion Cost</th>
                                                <th style="width:80px">Sample Cost</th>
                                            </tr>
                                            <tr style="background-color:lightyellow; padding-right:1em">
                                                <td style="text-align:right">Total</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'order40HCWithCostPrice' | currency : '' : 1}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'fobCostUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'fobAmountUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">100 %</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'deltaInUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{(salePerformance | sumFunc:'deltaInUSD') / (salePerformance | sumFunc:'fobCostUSD') * 100 | number:1}} %</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'deltaAfterAllInUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{(salePerformance | sumFunc:'deltaAfterAllInUSD') / (salePerformance | sumFunc:'fobCostUSD') * 100 | number:1}} %</td>
                                                <td style="text-align:right">100 %</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'commissionAmountUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'damagesCostInUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'lcCostAmount' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'interestAmount' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'transportationInUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'inspectionCostUSD' | currency : '$' : 0}}</td>
                                                <td style="text-align:right">{{salePerformance | sumFunc:'sampleCostUSD' | currency : '$' : 0}}</td>
                                            </tr>
                                            <tr ng-repeat="item in salePerformance | orderBy : '-netGrossMarginAfterCommissionPercent'" style="padding-right:1em">
                                                <td style="background-color:#FF7599">{{item.saleNM}}</td>
                                                <td style="text-align:right">{{item.order40HCWithCostPrice | currency:'':1}}</td>
                                                <td style="text-align:right">{{item.fobCostUSD | currency:'$':0}}</td>
                                                <td style="text-align:right; background-color:#FF7599">{{item.fobAmountUSD | currency:'$':0}}</td>
                                                <td style="text-align:right; background-color:#FF7599">{{item.fobAmountUSD / (salePerformance | sumFunc:'fobAmountUSD') * 100 | number : 1}} %</td>
                                                <td style="text-align:right">{{item.deltaInUSD | currency:'$':0}}</td>
                                                <td style="text-align:right">{{item.deltaPercent | number:1}} %</td>
                                                <td style="text-align:right; background-color:#FF7599">{{item.deltaAfterAllInUSD | currency:'$':0}}</td>
                                                <td style="text-align:right; background-color:#ebf1de">{{item.deltaAfterAllPercent | number:1}} %</td>
                                                <td style="text-align:right; background-color:darkorange">{{item.deltaAfterAllInUSD / (salePerformance | sumFunc:'deltaAfterAllInUSD') * 100  | number : 1}} %</td>

                                                <td style="text-align:right">{{item.commissionAmountUSD | currency:'$':0}}</td>
                                                <td style="text-align:right">{{item.damagesCostInUSD | currency:'$':0}}</td>
                                                <td style="text-align:right">{{item.lcCostAmount | currency:'$':0}}</td>
                                                <td style="text-align:right">{{item.interestAmount | currency:'$':0}}</td>

                                                <td style="text-align:right">{{item.transportationInUSD | currency:'$':0}}</td>
                                                <td style="text-align:right">{{item.inspectionCostUSD | currency:'$':0}}</td>
                                                <td style="text-align:right">{{item.sampleCostUSD | currency:'$':0}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </article>
</div>

