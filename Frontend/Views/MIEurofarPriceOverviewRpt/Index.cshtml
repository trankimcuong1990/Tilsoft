                                                <td style="text-align:right;">${{item.lcCostAmount | currency:'':2}}</td>
                                                <td style="text-align:right;">${{item.interestAmount | currency:'':2}}</td>
                                                <td style="text-align:right;">${{item.inspectionCostUSD | currency:'':2}}</td>
