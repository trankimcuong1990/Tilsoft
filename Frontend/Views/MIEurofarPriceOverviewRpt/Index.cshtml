                                                <td style="font-weight:700;text-align:right">${{method.getTotal(data.customerData,'lcCostAmount') | number : 2}}</td>
                                                <td style="font-weight:700;text-align:right">${{method.getTotal(data.customerData,'interestAmount') | number : 2}}</td>
                                                <td style="text-align:right;">${{method.getTotal(data.customerData,'inspectionCostUSD') | number : 2}}</td>
                                                <td style="text-align:right;">${{method.getTotal(data.customerData,'transportationInUSD') | number : 2}}</td>
