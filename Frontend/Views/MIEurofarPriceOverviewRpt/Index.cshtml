                                                <td style="text-align:right">{{item.toBeShippedQnt40HC | number : 2}}</td>
                                                <td style="text-align:right">{{item.toBeShippedContInPercentage | number : 2}}%</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
