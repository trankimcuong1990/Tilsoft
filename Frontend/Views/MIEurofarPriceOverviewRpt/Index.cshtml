                                                <td style="font-weight:700;text-align:right">{{method.getTotal(data.customerData,'saleAmountEUR') | currency : '&euro;' : 2}}</td>
                                                <td style="font-weight:700;text-align:right">${{method.getTotal(data.customerData,'saleAmountUSD') | number : 2}}</td>
