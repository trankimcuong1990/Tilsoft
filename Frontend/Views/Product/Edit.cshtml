                                                        <i class="fa fa-user"></i> {{data.confirmerName2}}
                                                    </a>
                                                    <span ng-show="(data.confirmedBy!=null && data.confirmerName2==null && data.confirmerName!=null)">{{data.confirmerName}}</span>
