                                            <div style="text-align:center;">
                                                Receive Notification
                                                <div style="border:none"><input type="checkbox" ng-click="factoryAccessService.selectnotification(item)" ng-model="factoryAccessService.checkvaluetruenotification" style="margin-left:40px;margin-top:-14px;"/></div>
                                            </div>
                                            
                                        </div>
                                        <div class="tilsoft-table-body" style="width: 850px;">
                                            <table>
                                                <tr ng-repeat="item in factoryAccessService.data | orderBy: 'factoryUD'">
