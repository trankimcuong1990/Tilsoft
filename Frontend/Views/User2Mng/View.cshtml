@Html.Partial("~/Views/User2Mng/view/_Profile.cshtml")

<div id="frmAccount" class="row" style="display: none;">
    <form name="frmAccount">
        <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="jarviswidget" id="wid-account-form" data-widget-colorbutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" role="widget">
                <header role="heading">
                    <h2>Edit Account</h2>
                </header>
                <div role="content">
                    <div class="widget-body no-padding">
                        <div class="smart-form">
                            <fieldset>
                                <div class="row">
                                    <section class="col col-6">
                                        <label class="label">Login Name</label>
                                        <label class="input">
                                            <span class="form-control">{{accountService.data.userName}}</span>
                                        </label>
                                    </section>
                                    <section class="col col-4">
                                        <label class="label">Last Login</label>
                                        <label class="input">
                                            <span class="form-control">{{accountService.data.lastLoginDate}}</span>
                                        </label>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-8">
                                        <label class="label">
                                            User Group
                                            <em ng-show="frmAccount.userGroup.$error.required" class="note note-error">(Required)</em>
                                        </label>
                                        <label class="select" ng-class="{'state-error': frmAccount.userGroup.$error.required}">
                                            <select name="userGroup" ng-model="accountService.data.userGroupID" ng-options="group.userGroupID as group.userGroupNM for group in userGroups" required>
                                                <option value=""></option>
                                            </select>
                                            <i></i>
                                        </label>
                                    </section>
                                    <section class="col col-2">
                                        <label class="label">&nbsp;</label>
                                        <label class="checkbox">
                                            <input type="checkbox" ng-model="accountService.data.isActivated" />
                                            <i></i>Active
                                        </label>
                                    </section>
                                    <section class="col col-2">
                                        <label class="label">&nbsp;</label>
                                        <label class="checkbox">
                                            <input type="checkbox" ng-model="accountService.data.isSuperUser" />
                                            <i></i>Super User
                                        </label>
                                    </section>
                                </div>
                            </fieldset>
                        </div>
                        <div class="widget-footer">
                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="accountService.update()"><i class="fa fa-check"></i> Save</a>
                            <a href="javascript:void(0)" class="btn btn-default btn-sm" ng-click="accountService.closeForm()"><i class="fa fa-times"></i> Cancel</a>
                        </div>
                    </div>
                </div>
            </div>
        </article>
    </form>
</div>

<div id="frmPermission" class="row" style="display: none;">
    <form name="frmPermission">
        <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="jarviswidget" id="wid-permission-form" data-widget-colorbutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" role="widget">
                <header role="heading">
                    <h2>Edit Permission</h2>
                </header>
                <div role="content">
                    <div class="widget-body no-padding">
                        <div class="smart-form">
                            <section>
                                <div avs-scroll grid-handler="gridHandler" class="tilsoft-table-wrapper">
                                    <div class="tilsoft-table">
                                        <div class="tilsoft-table-header" style="width: 890px;">
                                            <div style="width: 400px;">Module</div>
                                            <div style="width: 70px;">Read</div>
                                            <div style="width: 70px;">Create</div>
                                            <div style="width: 70px;">Update</div>
                                            <div style="width: 70px;">Delete</div>
                                            <div style="width: 70px;">Print</div>
                                            <div style="width: 70px;">Approve</div>
                                            <div>Reset</div>
                                        </div>
                                        <div class="tilsoft-table-body" style="width: 890px;">
                                            <table>
                                                <tr ng-repeat="item in permissionService.data | orderBy: 'displayOrder'" ng-class="{'parent-level':item.parentID == 0}">
                                                    <td style="width: 400px;">{{item.displayText}}</td>
                                                    <td style="width: 70px; text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canRead && !item.canReadEditable"></i>
                                                        <label ng-if="item.canReadEditable"><input type="checkbox" ng-model="item.canRead" /></label>
                                                    </td>
                                                    <td style="width: 70px; text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canCreate && !item.canCreateEditable"></i>
                                                        <label ng-if="item.canCreateEditable"><input type="checkbox" ng-model="item.canCreate" /></label>
                                                    </td>
                                                    <td style="width: 70px; text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canUpdate && !item.canUpdateEditable"></i>
                                                        <label ng-if="item.canUpdateEditable"><input type="checkbox" ng-model="item.canUpdate" /></label>
                                                    </td>
                                                    <td style="width: 70px; text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canDelete && !item.canDeleteEditable"></i>
                                                        <label ng-if="item.canDeleteEditable"><input type="checkbox" ng-model="item.canDelete" /></label>
                                                    </td>
                                                    <td style="width: 70px; text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canPrint && !item.canPrintEditable"></i>
                                                        <label ng-if="item.canPrintEditable"><input type="checkbox" ng-model="item.canPrint" /></label>
                                                    </td>
                                                    <td style="width: 70px; text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canApprove && !item.canApproveEditable"></i>
                                                        <label ng-if="item.canApproveEditable"><input type="checkbox" ng-model="item.canApprove" /></label>
                                                    </td>
                                                    <td style="text-align: center;">
                                                        <i title="Inherited from the usergroup permission" class="fa fa-check-square-o" ng-if="item.canReset && !item.canResetEditable"></i>
                                                        <label ng-if="item.canResetEditable"><input type="checkbox" ng-model="item.canReset" /></label>
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                        <div class="widget-footer">
                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="permissionService.update()"><i class="fa fa-check"></i> Save</a>
                            <a href="javascript:void(0)" class="btn btn-default btn-sm" ng-click="permissionService.closeForm()"><i class="fa fa-times"></i> Cancel</a>
                        </div>
                    </div>
                </div>
            </div>
        </article>
    </form>
</div>

<div id="frmPassword" class="row" style="display: none;">
    <form name="frmPassword">
        <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="jarviswidget" id="wid-permission-form" data-widget-colorbutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" role="widget">
                <header role="heading">
                    <h2>Change Password</h2>
                </header>
                <div role="content">
                    <div class="widget-body no-padding">
                        <div class="smart-form">
                            <fieldset>
                                <div class="row">
                                    <section class="col col-6">
                                        <label class="label">
                                            New Password
                                            <em ng-show="frmPassword.password.$error.required" class="note note-error">(Required)</em>
                                            <em ng-show="frmPassword.password.$error.minlength" class="note note-error">(Password must be at least 7 chars length)</em>
                                        </label>
                                        <label class="input" ng-class="{'state-error': frmPassword.password.$error.required || frmPassword.password.$error.minlength}">
                                            <input type="password" ng-model="passwordService.data.newPassword" name="password" required ng-minlength="7" />
                                        </label>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-6">
                                        <label class="label">
                                            Confirmation
                                            <em ng-show="frmPassword.confirmation.$error.required" class="note note-error">(Required)</em>
                                            <em ng-show="frmPassword.confirmation.$error.minlength" class="note note-error">(Password must be at least 7 chars length)</em>
                                        </label>
                                        <label class="input" ng-class="{'state-error': frmPassword.confirmation.$error.required || frmPassword.confirmation.$error.minlength}">
                                            <input type="password" ng-model="passwordService.data.confirmation" name="confirmation" required ng-minlength="7" />
                                        </label>
                                    </section>
                                </div>
                            </fieldset>
                        </div>
                        <div class="widget-footer">
                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="passwordService.update()"><i class="fa fa-check"></i> Save</a>
                            <a href="javascript:void(0)" class="btn btn-default btn-sm" ng-click="passwordService.closeForm()"><i class="fa fa-times"></i> Cancel</a>
                        </div>
                    </div>
                </div>
            </div>
        </article>
    </form>
</div>

<div id="frmFactoryAccess" class="row" style="display: none;">
    <form name="frmFactoryAccess">
        <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="jarviswidget" id="wid-permission-form" data-widget-colorbutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" role="widget">
