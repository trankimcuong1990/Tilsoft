@{
    ViewBag.Title = "User Profile";
    ViewBag.Icon = "fa-user";
    ViewBag.Module = "User Profile";
}

<div class="row tilsoft-profile" id="main-form">
    <div class="col-sm-12">
        <div id="profile-section" class="well well-sm" style="background: #fff;">
            <div class="row">
                <div class="col-sm-12">
                    <div style="background: url('@System.Web.VirtualPathUtility.ToAbsolute("~/img/demo/s1.jpg")') top left no-repeat; height: 150px; background-size: cover;"></div>
                    <div style="position: relative;">
                        <div class="air air-bottom-right" style="margin: 10px; background: rgba(0,0,0,0.5); color: #fff; font-size: 25px; padding: 10px 20px;">
                            <i class="fa fa-briefcase"></i> {{data.internalCompanyNM}}
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="profile-image">
                                <a href="{{data.fileLocation}}" data-featherlight="image">
                                    <img style="width: 95%;" ng-src="{{data.thumbnailLocation}}" />
                                </a>

                                <a href="#" class="btn btn-success" style="margin-top: 10px; width: 100%;" ng-click="event.profile_openForm()"><i class="fa fa-pencil"></i> Edit</a>
                            </div>
                        </div>
                        <div class="col-sm-10">
                            <h1>
                                {{data.employeeNM}} (Calling name: {{data.employeeFirstNM}})
                                <br />
                                <span class="small">{{data.jobTitle}} (Level: {{data.jobLevelNM}})</span>
                            </h1>
                            <div class="row">
                                <div class="col-sm-4">
                                    <ul class="list-unstyled">
                                        <li>
                                            <p class="text-muted">
                                                <i class="fa fa-phone"></i>
                                                <a href="#">{{data.telephone1}}</a>
                                                <br />
                                                <a href="#">{{data.telephone2}}</a>
                                                &nbsp;
                                            </p>
                                        </li>
                                        <li>
                                            <p class="text-muted">
                                                <i class="fa fa-envelope"></i>
                                                <a href="mailto:{{data.email1}}">{{data.email1}}</a>
                                                <br />
                                                <a href="mailto:{{data.email2}}">{{data.email2}}</a>
                                                &nbsp;
                                            </p>
                                        </li>
                                        <li>
                                            <p class="text-muted">
                                                <i class="fa fa-skype"></i>
                                                <a href="skype:{{data.skypeID}}:chat">{{data.skypeID}}</a>
                                                &nbsp;
                                            </p>
                                        </li>
                                        <li>
                                            <p class="text-muted">
                                                <i class="fa fa-calendar-o"></i>
                                                <span class="txt-color-darken">{{data.dateOfBirth}}</span>
                                                &nbsp;
                                            </p>
                                        </li>
                                        <li>
                                            <p class="text-muted">
                                                <i class="fa fa-map-marker"></i>
                                                <span class="txt-color-darken">{{data.locationNM}}</span>
                                                &nbsp;
                                            </p>
                                        </li>
                                        <li>
                                            <p class="text-muted">
                                                <i class="fa fa-file-word-o"></i>
                                                Resume: <a target="_blank" href="{{data.cvFileLocation}}">{{data.cvFileName}}</a>
                                            </p>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-sm-8">
                                    <table class="table table-bordered table-striped">
                                        <tbody>
                                            <tr>
                                                <th><i class="fa fa-calendar-o"></i> Joined Date</th>
                                                <td>{{data.dateStart}}</td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-paper-plane-o"></i> Report To</th>
                                                <td>
                                                    <a ng-if="data.reportToID" href="@Url.Action("Detail", "EmployeeMng", new { id = UrlParameter.Optional })/{{data.reportToID}}" data-featherlight="iframe" data-featherlight-iframe-allowfullscreen="true" data-featherlight-iframe-width="800" data-featherlight-iframe-height="600">
                                                        {{data.managerName}}
                                                    </a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-briefcase"></i> Department</th>
                                                <td>{{data.internalDepartmentNM}}</td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-briefcase"></i> Factory</th>
                                                <td>
                                                    <div class="bootstrap-tagsinput" style="border: none; padding: 0px;">
                                                        <span class="tag label label-info" ng-repeat="item in data.employeeFactories">
                                                            {{item.factoryUD}}
                                                        </span>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-certificate"></i> English</th>
                                                <td>{{data.english}}</td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-gears"></i> TILSOFT Usage</th>
                                                <td>TILSOFT USAGE</td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-gears"></i> TILSOFT Licensed From</th>
                                                <td>{{data.licensedDate}}</td>
                                            </tr>
                                            <tr>
                                                <th><i class="fa fa-random"></i> Other contact method</th>
                                                <td>{{data.otherContactInfo}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-6">
                                    <p class="font-md">
                                        <i class="fa fa-suitcase"></i> Job Description
                                    </p>
                                    <p ng-bind-html="dataService.formatRemark(data.jobDescription)"></p>
                                </div>
                                <div class="col-sm-6">
                                    <p class="font-md">
                                        <i class="fa fa-tasks"></i> Daily Task
                                    </p>
                                    <p ng-bind-html="dataService.formatRemark(data.dailyTask)"></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="account-section" class="well well-sm" style="background: #fff;">
            <h1>TILSOFT Account</h1>
            <hr />
            <div class="row">
                <div class="col-sm-2">
                    <div class="profile-image" style="top: auto; padding-top: 0px;">
                        <a href="#" class="btn btn-success" style="width: 100%;" ng-click="accountService.openForm()"><i class="fa fa-pencil"></i> Edit</a>
                        <a href="#" class="btn btn-default" style="margin-top: 10px; width: 100%;" ng-click="passwordService.openForm()"><i class="fa fa-lock"></i> Change PW</a>
                        <a href="#" class="btn btn-default" style="margin-top: 10px; width: 100%;" ng-click="passwordService.forceChangePassword()"><i class="fa fa-lock"></i> Force change PW</a>
                        <a href="javascript:void(0)" class="btn btn-default" style="width: 100%; margin-top: 10px;" ng-click="event.generateAPI()"><i class="fa fa-exchange"></i> Generate API</a>
                    </div>
                </div>
                <div class="col-sm-10">
                    <table class="table table-bordered table-striped">
                        <tbody>
                            <tr>
                                <th><i class="fa fa-user"></i> Account</th>
                                <td>{{data.userName}}</td>
                            </tr>
                            <tr>
                                <th><i class="fa fa-exchange"></i> Secrete key</th>
                                <td>{{data.apiKey}}</td>
                            </tr>
                            <tr>
                                <th><i class="fa fa-users"></i> User Group</th>
                                <td>{{data.userGroupNM}}</td>
                            </tr>
                            <tr>
                                <th><i class="fa fa-smile-o"></i> Active </th>
                                <td><i class="fa fa-check-square-o" ng-if="data.isActivated"></i></td>
                            </tr>
                            <tr>
                                <th><i class="fa fa-trophy"></i> Super User </th>
                                <td><i class="fa fa-check-square-o" ng-if="data.isSuperUser"></i></td>
                            </tr>
                            <tr>
                                <th><i class="fa fa-calendar-o"></i> Last Login</th>
                                <td>{{data.lastLoginDate}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <div id="permission-section" class="well well-sm" style="background: #fff;">
            <h1>Access Permission</h1>
            <hr />
            <div class="row">
                <div class="col-sm-2">
                    <div class="profile-image" style="top: auto; padding-top: 0px;">
                        <a href="#" class="btn btn-success" style="width: 100%;" ng-click="permissionService.openForm()"><i class="fa fa-pencil"></i> Edit</a>
                    </div>
                </div>
                <div class="col-sm-10">
                    <div avs-scroll grid-handler="gridHandler" class="tilsoft-table-wrapper">
                        <div class="tilsoft-table">
                            <div class="tilsoft-table-header" style="width: 890px;">
                                <div style="width: 400px;">Module</div>
                                <div style="width: 70px;">Read</div>
                                <div style="width: 70px;">Create</div>
                                <div style="width: 70px;">Update</div>
                                <div style="width: 70px;">Delete</div>
                                <div style="width: 70px;">Print</div>
                                <div style="width: 70px;">Approve</div>
                                <div>Reset</div>
                            </div>
                            <div class="tilsoft-table-body" style="width: 890px;">
                                <table>
                                    <tr ng-repeat="item in data.permissions | orderBy: 'displayOrder'" ng-class="{'parent-level':item.parentID == 0}">
                                        <td style="width: 400px;">{{item.displayText}}</td>
                                        <td style="width: 70px; text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canRead"></i></td>
                                        <td style="width: 70px; text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canCreate"></i></td>
                                        <td style="width: 70px; text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canUpdate"></i></td>
                                        <td style="width: 70px; text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canDelete"></i></td>
                                        <td style="width: 70px; text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canPrint"></i></td>
                                        <td style="width: 70px; text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canApprove"></i></td>
                                        <td style="text-align: center;"><i class="fa fa-check-square-o" ng-if="item.canReset"></i></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="factory-access-section" class="well well-sm" style="background: #fff;">
            <h1>Factory Access</h1>
            <hr />
            <div class="row">
                <div class="col-sm-2">
                    <div class="profile-image" style="top: auto; padding-top: 0px;">
                        <a href="#" class="btn btn-success" style="width: 100%;" ng-click="factoryAccessService.openForm()"><i class="fa fa-pencil"></i> Edit</a>
                    </div>
                </div>
                <div class="col-sm-10">
                    <div avs-scroll grid-handler="gridHandler" class="tilsoft-table-wrapper">
                        <div class="tilsoft-table">
                            <div class="tilsoft-table-header" style="width: 850px;">
                                <div style="width: 400px;">Factory</div>
                                <div style="width: 70px;">Access</div>
                                <div style="text-align: center;">Receive Notification</div>
                            </div>
                            <div class="tilsoft-table-body" style="width: 850px;">
                                <table>
                                    <tr ng-repeat="item in data.factoryAccesses | orderBy: 'factoryUD'">
                                        <td style="width: 400px;">{{item.factoryUD}}</td>
                                        <td style="width: 70px; text-align: center;">
                                            <i class="fa fa-check-square-o" ng-if="item.canAccess"></i>
                                        </td>
                                        <td style="text-align: center;">
                                            <i class="fa fa-check-square-o" ng-if="item.canReceiveNotification"></i>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

