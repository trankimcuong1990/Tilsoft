                                    <div class="tilsoft-table">
                                        <div class="tilsoft-table-header" style="width: 850px;">
                                            <div style="width: 400px;">
                                                Factory
                                            </div>
                                            <div style="width: 70px;">
                                                Access
                                                <label style="margin-left:11px;border:none"><input type="checkbox" ng-click="factoryAccessService.selectAll(item)" ng-model="factoryAccessService.checkvaluetrue" /></label>
