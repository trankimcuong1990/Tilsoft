@{
    if (ViewBag.ID == 0)
    {
        ViewBag.Title = "Create New Eurofar Commercial Invoice" + " -  " + "{{ecommercialinvoice.eCommercialInvoiceData.typeOfInvoiceText}}";
    }
    else
    {
        ViewBag.Title = "Edit Eurofar Commerical Invoice" + " -  " + "{{ecommercialinvoice.eCommercialInvoiceData.typeOfInvoiceText}}";
    }

    ViewBag.Module = "Eurofar Commercial Invoice";
}
<style>
    .modal-dialog {
        margin-top: 10% !important;
    }
</style>
<form class="row" name="editForm" id="main-form">

    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12" ng-show="ecommercialinvoice.eCommercialInvoiceData.typeOfInvoice!=5">
        <div class="jarviswidget" id="wid-id-general" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
            <header>
                <ul class="nav nav-tabs pull-left in">
                    <li class="active">
                        <a data-toggle="tab" href="#generalTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet"> General Information </span> </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#clientTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet">Client</span> </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#factoryInvoiceTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet">Factory Invoice Info</span> </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#costDescriptionTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet">Cost Description</span> </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#creditNoteTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet">Credit Note</span> </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#warehouseImportTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet">Warehouse return</span> </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#trackingTab"> <i class="fa fa-database"></i> <span class="hidden-mobile hidden-tablet">Tracking</span> </a>
                    </li>
                </ul>
            </header>
            <div>
                <div class="widget-body">
                    <div class="tab-content">
                        <div class="tab-pane active" id="generalTab">
                            <div class="smart-form">
                                <fieldset>
                                    <div class="row">
                                        <section class="col col-2">
                                            <label>Invoice No Tilsoft</label>
                                            <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.invoiceNo}}</span>
                                        </section>

                                        <section class="col col-2">
                                            <label>Status</label>
                                            <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.isConfirmedText}}</span>
                                        </section>

                                        <section class="col col-2">
                                            <label>Inv.No at client system</label>
                                            <input class="form-control" type="text" name="clientInvoiceNo" ng-model="ecommercialinvoice.eCommercialInvoiceData.clientInvoiceNo">
                                        </section>

                                        <section class="col col-1">
                                            <label>REF No</label>
                                            <input class="form-control" type="text" name="refNo" ng-model="ecommercialinvoice.eCommercialInvoiceData.refNo">
                                        </section>

                                        <section class="col col-1">
                                            <label>LC REF No</label>
                                            <input class="form-control" type="text" name="lcRefNo" ng-model="ecommercialinvoice.eCommercialInvoiceData.lcRefNo">
                                        </section>

                                        <section class="col col-2">
                                            <label>Remark</label>
                                            <input class="form-control" type="text" name="remark" ng-model="ecommercialinvoice.eCommercialInvoiceData.remark" ng-required="ecommercialinvoice.eCommercialInvoiceData.typeOfInvoice==4">
                                            <span class="help-block" ng-show="editForm.remark.$error.required"><i class="fa fa-warning"></i>Remark is require in case type of invoice is CREIDT NOTE</span>
                                        </section>

                                        <section class="col col-2">
                                            <label>Payment status / <a href="javascript:void(0);" class="btn btn-xs  btn-primary" style="font-size: 10px; margin-bottom: 4px" ng-click="setDonePayment($event)">{{ecommercialinvoice.eCommercialInvoiceData.isDonePaymentLabel}}</a></label>
                                            <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.isDonePaymentText}}</span>
                                        </section>


                                    </div>
                                    <div class="row">

                                        <section class="col col-2">
                                            <label>Invoice Date</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control datepicker" data-dateformat="dd/mm/yy" ng-model="ecommercialinvoice.eCommercialInvoiceData.invoiceDateFormated">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </section>

                                        <section class="col col-2">
                                            <label>Account No</label>
                                            <select class="form-control" name="accountNo" ng-model="ecommercialinvoice.eCommercialInvoiceData.accountNo" ng-options="item.accountNo as item.accountNM for item in ecommercialinvoice.turnOverLedgerAccounts">
                                                <option value=""></option>
                                            </select>
                                        </section>

                                        <section class="col col-2">
                                            <label>Delivery Term</label>
                                            <select class="form-control" name="deliveryTermID" ng-model="ecommercialinvoice.eCommercialInvoiceData.deliveryTermID" ng-options="item.deliveryTermID as item.deliveryTermNM for item in ecommercialinvoice.deliveryTerms" ng-change="selectedDelivery(ecommercialinvoice.eCommercialInvoiceData.deliveryTermID)">
                                                <option value=""></option>
                                            </select>
                                        </section>

                                        <section class="col col-2">
                                            <label>Delivery Term Text</label>
                                            <input class="form-control" type="text" id="deliveryTerm" ng-model="ecommercialinvoice.eCommercialInvoiceData.deliveryTerm">
                                        </section>

                                        <section class="col col-2">
                                            <label>Payment Term</label>
                                            <select class="form-control" name="paymentTermID" ng-model="ecommercialinvoice.eCommercialInvoiceData.paymentTermID" ng-options="item.paymentTermID as item.paymentTermNM for item in ecommercialinvoice.paymentTerms" ng-change="selectedPaymentTerm(ecommercialinvoice.eCommercialInvoiceData.paymentTermID)">
                                                <option value=" "></option>
                                            </select>
                                        </section>

                                        <section class="col col-2">
                                            <label>Payment Term Text</label>
                                            <input class="form-control" type="text" id="paymentTerm" ng-model="ecommercialinvoice.eCommercialInvoiceData.paymentTerm">
                                        </section>
                                    </div>
                                    <div class="row">
                                        <section class="col col-1">
                                            <label>Season</label>
                                            <select class="form-control" name="season" ng-model="ecommercialinvoice.eCommercialInvoiceData.season" ng-options="item.seasonValue as item.seasonText for item in ecommercialinvoice.seasons" ng-disabled="ecommercialinvoice.eCommercialInvoiceData.eCommercialInvoiceID>0" required>
                                                <option value=""></option>
                                            </select>
                                            <span class="help-block" ng-show="editForm.season.$error.required"><i class="fa fa-warning"></i>Season is required.</span>
                                        </section>

                                        <section class="col col-3">
                                            <label>Conditions</label>
                                            <input class="form-control" type="text" name="conditions" ng-model="ecommercialinvoice.eCommercialInvoiceData.conditions">
                                        </section>

                                        <section class="col col-1">
                                            <label>Currency</label>
                                            <select class="form-control" name="currency" ng-model="ecommercialinvoice.eCommercialInvoiceData.currency" ng-options="item.currencyValue as item.currencyText for item in ecommercialinvoice.currency" required>
                                                <option value=""></option>
                                            </select>
                                            <span class="help-block" ng-show="editForm.currency.$error.required"><i class="fa fa-warning"></i>Currency is required.</span>
                                        </section>

                                        <section class="col col-1">
                                            <label>VAT Rate</label>
                                            <select class="form-control" name="vatRate" ng-model="ecommercialinvoice.eCommercialInvoiceData.vatRate" ng-options="item.vatPercentValue as item.vatPercentText for item in ecommercialinvoice.vatPercents" required>
                                                <option value=""></option>
                                            </select>
                                            <span class="help-block" ng-show="editForm.vatRate.$error.required"><i class="fa fa-warning"></i>VAT is required.</span>
                                        </section>

                                        <section class="col col-2">
                                            <label>Net Amount</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">{{ecommercialinvoice.eCommercialInvoiceData.currency == 'USD' ? '$' : ''}} {{ecommercialinvoice.eCommercialInvoiceData.currency == 'EUR' ? '€' : ''}}</span>
                                                <span class="form-control" style="text-align:right">{{netRecal() | number : 2}}</span>
                                            </div>
                                        </section>

                                        <section class="col col-2">
                                            <label>VAT Amount</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">{{ecommercialinvoice.eCommercialInvoiceData.currency == 'USD' ? '$' : ''}} {{ecommercialinvoice.eCommercialInvoiceData.currency == 'EUR' ? '€' : ''}}</span>
                                                <span class="form-control" style="text-align:right">{{vatRecal() | number : 2}}</span>
                                            </div>
                                        </section>

                                        <section class="col col-2">
                                            <label>Total Amount</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">{{ecommercialinvoice.eCommercialInvoiceData.currency == 'USD' ? '$' : ''}} {{ecommercialinvoice.eCommercialInvoiceData.currency == 'EUR' ? '€' : ''}}</span>
                                                <span class="form-control" style="text-align:right">{{totalRecal() | number : 2}}</span>
                                            </div>
                                        </section>

                                    </div>
                                </fieldset>
                            </div>



                        </div>

                        <div class="tab-pane" id="clientTab">
                            <div class="row">

                                <div class="col-sm-2">
                                    <label>Client Code</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.clientUD}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label>Client Name</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.clientNM}}</span>
                                </div>


                                <div class="col-sm-2">
                                    <label>VAT Code</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.vatNo}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label>Telephone</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.telephone}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label>Fax</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.fax}}</span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <label>Client Address</label>
                                    <textarea id="clientAddress" ng-model="ecommercialinvoice.eCommercialInvoiceData.clientAddress" class="form-control" rows="4"></textarea>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="factoryInvoiceTab">
                            <div class="row">
                                <div class="col-sm-12 m-bottom-15">
                                    <div id="topDescriptionTable" class="table-responsive">
                                        <table class="table table-bordered table-hover custom-table dataTable">
                                            <thead>
                                                <tr>
                                                    <th data-colname="invoiceNo" style="width: 80px;">INVOICE.NO</th>
                                                    <th data-colname="factoryInvoiceNo" style="width: 150px;">FACTORY INVOICE.NO</th>
                                                    <th data-colname="blNo" style="width: 150px;">BL NO</th>
                                                    <th data-colname="blNo" style="width: 70px;">ETD</th>
                                                    <th data-colname="etA" style="width: 70px;">ETA1</th>
                                                    <th data-colname="etA2" style="width: 70px;">ETA2</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="item in ecommercialinvoice.eCommercialInvoiceData.bookings">
                                                    <td>
                                                        <a href="@Url.Action("Edit", "PurchasingInvoice")/{{item.purchasingInvoiceID}}" target="_blank">{{item.invoiceNo}}</a>
                                                    </td>
                                                    <td>
                                                        <a href="@Url.Action("Edit", "PurchasingInvoice")/{{item.purchasingInvoiceID}}" target="_blank">{{item.factoryInvoiceNo}}</a>
                                                    </td>
                                                    <td>
                                                        <a href="@Url.Action("Edit", "Booking2Mng")/{{item.bookingID}}" target="_blank">{{item.blNo}}</a>
                                                    </td>
                                                    <td>
                                                        {{item.etd}}
                                                    </td>
                                                    <td>
                                                        {{item.eta}}
                                                    </td>
                                                    <td>
                                                        {{item.etA2}}
                                                    </td>

                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="costDescriptionTab">
                            <div class="row">
                                <div class="col-sm-6 m-bottom-15">
                                    <div>
                                        <a class="btn btn-primary btn-xs font-11" href="javascript:void(0);" ng-click="addDescription($event,'TOP')" style="margin-bottom: 10px;"><i class="fa fa-plus"></i>Add top description</a>
                                    </div>
                                    <div id="topDescriptionTable" class="table-responsive">
                                        <table class="table table-bordered table-hover custom-table dataTable">
                                            <thead>
                                                <tr>
                                                    <th style="width: 90px;"></th>
                                                    <th class="sorting" data-colname="rowIndex" style="width: 40px;">INDEX</th>
                                                    <th class="sorting" data-colname="costType" style="width: 80px;">COST TYPE</th>
                                                    <th class="sorting_asc" data-colname="description">TOP DESCRIPTION</th>
                                                    <th class="sorting" data-colname="percent" style="width: 40px;text-align:center">COST %</th>
                                                    <th class="sorting" data-colname="totalAmount" style="width: 100px;">AMOUNT</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="top_item in ecommercialinvoice.eCommercialInvoiceData.eCommercialInvoiceDescriptions | filter : { position: 'TOP' } | orderBy : 'rowIndex'">
                                                    <td style="text-align: center; ">
                                                        <a href="#" class="btn btn-primary btn-xs font-11" title="Insert" ng-click="insertDescription($event, 'TOP',top_item.rowIndex)"><i class="fa fa-level-down"></i> Insert</a>
                                                        <a href="#" class="btn btn-danger btn-xs font-11" title="Delete" ng-click="removeDescription($event, top_item.eCommercialInvoiceDescriptionID)"><i class="fa fa-times"></i>Del</a>
                                                    </td>

                                                    <td>
                                                        <input class="form-control" type="text" name="rowIndex" ng-model="top_item.rowIndex">
                                                    </td>

                                                    <td>
                                                        <select class="form-control" name="costType" ng-model="top_item.costType" ng-options="sItem.costTypeValue as sItem.costTypeText for sItem in ecommercialinvoice.costTypes">
                                                            <option value=""></option>
                                                        </select>
                                                    </td>

                                                    <td>
                                                        <input class="form-control" type="text" name="description" ng-model="top_item.description">
                                                    </td>
                                                    <td><input class="form-control" type="text" name="percent" ng-model="top_item.percent" ng-keyup="selectedPercentCost(top_item)"> </td>
                                                    <td>
                                                        <div class="input-group">
                                                            <span class="input-group-addon">{{ecommercialinvoice.eCommercialInvoiceData.currency == 'USD' ? '$' : ''}} {{ecommercialinvoice.eCommercialInvoiceData.currency == 'EUR' ? '€' : ''}}</span>
                                                            <input type="text" class="form-control furnindo-currency" ng-model="top_item.totalAmount" style="text-align:right" ng-currency fraction="2" currency-symbol="" />
                                                        </div>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                                <div class="col-sm-6 m-bottom-15">
                                    <div>
                                        <a class="btn btn-primary btn-xs font-11" href="javascript:void(0);" ng-click="addDescription($event,'BOTTOM')" style="margin-bottom: 10px;"><i class="fa fa-plus"></i>Add bottom description</a>
                                    </div>
                                    <div id="bottomDescriptionTable" class="table-responsive">
                                        <table class="table table-bordered table-hover custom-table dataTable">
                                            <thead>
                                                <tr>
                                                    <th style="width: 90px;"></th>
                                                    <th class="sorting" data-colname="rowIndex" style="width: 40px;">INDEX</th>
                                                    <th class="sorting" data-colname="costType" style="width: 80px;">COST TYPE</th>
                                                    <th class="sorting" data-colname="description">BOTTOM DESCRIPTION</th>
                                                    <th class="sorting" data-colname="percent" style="width: 40px;text-align:center">COST %</th>
                                                    <th class="sorting" data-colname="totalAmount" style="width: 100px;">AMOUNT</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="top_item in ecommercialinvoice.eCommercialInvoiceData.eCommercialInvoiceDescriptions | filter : { position: 'BOTTOM' } | orderBy : 'rowIndex'">

                                                    <td style="text-align: center; ">
                                                        <a href="#" class="btn btn-primary btn-xs font-11" title="Insert" ng-click="insertDescription($event, 'BOTTOM',top_item.rowIndex)"><i class="fa fa-level-down"></i> Insert</a>
                                                        <a href="#" class="btn btn-danger btn-xs font-11" title="Delete" ng-click="removeDescription($event, top_item.eCommercialInvoiceDescriptionID)"><i class="fa fa-times"></i>Del</a>
                                                    </td>
                                                    <td><input class="form-control" type="text" name="rowIndex" ng-model="top_item.rowIndex"> </td>
                                                    <td>
                                                        <select class="form-control" name="costType" ng-model="top_item.costType" ng-options="sItem.costTypeValue as sItem.costTypeText for sItem in ecommercialinvoice.costTypes">
                                                            <option value=""></option>
                                                        </select>
                                                    </td>
                                                    <td><input class="form-control" type="text" name="description" ng-model="top_item.description"> </td>
                                                    <td><input class="form-control" type="text" name="percent" ng-model="top_item.percent" ng-keyup="selectedPercentCost(top_item)"> </td>
                                                    <td>
                                                        <div class="input-group">
                                                            <span class="input-group-addon">{{ecommercialinvoice.eCommercialInvoiceData.currency == 'USD' ? '$' : ''}} {{ecommercialinvoice.eCommercialInvoiceData.currency == 'EUR' ? '€' : ''}}</span>
                                                            <input type="text" class="form-control furnindo-currency" ng-model="top_item.totalAmount" style="text-align:right" ng-currency fraction="2" currency-symbol="" />
                                                        </div>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="creditNoteTab">
                            <div class="row">
                                <div class="col-sm-12 m-bottom-15">
                                    <div id="topDescriptionTable" class="table-responsive">
                                        <table class="table table-bordered table-hover custom-table dataTable">
                                            <thead>
                                                <tr>
                                                    <td style="width:50px"></td>
                                                    <th data-colname="CreditNoteNo" style="width: 80px;">CREDIT NOTE NO.</th>
                                                    <th data-colname="InvoiceDate" style="width: 80px;">CREDIT NOTE DATE</th>
                                                    <th data-colname="RefNo" style="width: 150px;">REF NO.</th>
                                                    <th data-colname="CreatorName" style="width: 100px;">CREATE BY</th>
                                                    <th data-colname="CreatedDate" style="width: 80px;">CREATE DATE</th>
                                                    <th data-colname="UpdatorName" style="width: 100px;">UPDATED BY</th>
                                                    <th data-colname="UpdatedDate" style="width: 80px;">UPDATED DATE</th>
                                                    <th data-colname="Remark">REMARK</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="item in ecommercialinvoice.eCommercialInvoiceData.creditNotes">
                                                    <td style="text-align:center">
                                                        <a href="@Url.Action("CreditNote", "ECommercialInvoice")/{{item.creditNoteID}}?eCommercialInvoiceID=0" title="Edit" class="btn btn-primary btn-xs font-11" target="_blank"><i class="fa fa-pencil"></i> Edit</a>
                                                    </td>
                                                    <td>
                                                        {{item.creditNoteNo}}
                                                    </td>
                                                    <td>
                                                        {{item.invoiceDate}}
                                                    </td>
                                                    <td>
                                                        {{item.refNo}}
                                                    </td>
                                                    <td>
                                                        {{item.creatorName}}
                                                    </td>
                                                    <td>
                                                        {{item.createdDate}}
                                                    </td>
                                                    <td>
                                                        {{item.updatorName}}
                                                    </td>
                                                    <td>
                                                        {{item.updatedDate}}
                                                    </td>
                                                    <td>
                                                        {{item.remark}}
                                                    </td>

                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="warehouseImportTab">
                            <div class="row">
                                <div class="col-sm-12 m-bottom-15">
                                    <div class="tilsoft-table">
                                        <div class="tilsoft-table-body" style="width: 100%; overflow-y: hidden; height: auto; margin-top: 0px;">
                                            <table>
                                                <tr>
                                                    <th data-colname="invoiceNo" style="width: 80px;"></th>
                                                    <th data-colname="invoiceNo" style="width: 150px;">Warehouse Import Receipt No</th>
                                                    <th data-colname="factoryInvoiceNo" style="width: 150px;">Import Date</th>
                                                    <th data-colname="blNo">Import Type</th>
                                                </tr>
                                                <tbody ng-repeat="item in ecommercialinvoice.eCommercialInvoiceData.warehouseImports">
                                                    <tr>
                                                        <td style="text-align:center">
                                                            <a href="@Url.Action("Return", "WarehouseImport")/{{item.warehouseImportID}}" title="Open warehouse import" class="btn btn-primary btn-xs font-11" target="_blank"><i class="fa fa-folder-open">View</i></a>
                                                        </td>
                                                        <td style="text-align:center">
                                                            {{item.receiptNo}}
                                                        </td>
                                                        <td style="text-align:center">
                                                            {{item.importedDate}}
                                                        </td>
                                                        <td style="text-align:center">
                                                            {{item.importTypeNM}}
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="trackingTab">

                            <div class="row">

                                <div class="col-sm-2">
                                    <label>Created By</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.creatorName}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label>Created Date</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.createdDateFormated}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label>Updated By</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.updatorName}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label>Updated Date</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.updatedDateFormated}}</span>
                                </div>


                            </div>

                            <div class="row" ng-if="ecommercialinvoice.eCommercialInvoiceData.isConfirmed==true || ecommercialinvoice.eCommercialInvoiceData.isConfirmed==false">
                                <div class="col-sm-2">
                                    <label ng-if="ecommercialinvoice.eCommercialInvoiceData.isConfirmed==true">Confirmed By</label><label ng-if="ecommercialinvoice.eCommercialInvoiceData.isConfirmed==false">Un-Confirmed By</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.confirmerName}}</span>
                                </div>

                                <div class="col-sm-2">
                                    <label ng-if="ecommercialinvoice.eCommercialInvoiceData.isConfirmed==true">Confirmed Date</label><label ng-if="ecommercialinvoice.eCommercialInvoiceData.isConfirmed==false">Un-Confirmed Date</label>
                                    <span class="form-control">{{ecommercialinvoice.eCommercialInvoiceData.confirmedDateFormated}}</span>
                                </div>
                            </div>

