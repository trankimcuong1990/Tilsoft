            <a href="#" class="btn btn-default @(Frontend.Helper.AuthHelper.GetActionButtonStatus(ViewBag.ModuleCode, Frontend.Helper.ActionEnum.Approve, HttpContext.Current)) {{ecommercialinvoice.eCommercialInvoiceData.isConfirmed==true?'' : 'disabled'}}" title="Create credit note" ng-click="createCreditNote($event)" style="color:#0026ff"><i class="fa fa-undo"></i></a>
        </li>

        <li class="sparks-info">
            <a href="#" class="btn btn-default @(Frontend.Helper.AuthHelper.GetActionButtonStatus(ViewBag.ModuleCode, Frontend.Helper.ActionEnum.Approve, HttpContext.Current)) {{ecommercialinvoice.eCommercialInvoiceData.typeOfInvoice==4?'' : 'disabled'}}" title="Return product" ng-click="returnGoodsForm.event.load($event)" style="color:#0026ff"><i class="fa fa-reply"></i></a>
        </li>

    </ul>
}

