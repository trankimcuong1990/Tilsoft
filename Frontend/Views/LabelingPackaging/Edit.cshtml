<div id="remarkForm" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" style="z-index: 1400;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="myModalLabel">Edit remark</h4>
            </div>
            <div class="modal-body">
                <div class="smart-form">
                    <fieldset>
                        <div class="row">
                            <div>
                                <a href="javascript:void(0);" ng-click="event.addRemark($event)"><b><u>Add new remark</u></b></a>
                            </div>
                            <div id="contract" class="tilsoft-table-wrapper">
                                <div class="tilsoft-table">
                                    <div class="tilsoft-table-header" style="width: 450px;height:auto;">
                                        <div style="width: 50px;"></div>
                                        <div style="width: 400px;">Remark</div>
                                        <div style="width: 100px;">Remark File</div>
                                        <div style="width: 100px;">Updated By</div>
                                        <div>Updated Date</div>
                                    </div>
                                    <div class="tilsoft-table-body" style="width: 450px; height:auto;">
                                        <table>
                                            <tr ng-repeat="item in data.labelingPackagingRemarks">
                                                <td style="width: 49px; text-align: center;"><a href="#" title="Delete" ng-click="event.removeRemark($event, item.labelingPackagingRemarkID)"><i class="fa fa-times-circle grid-button"></i></a></td>
                                                <td style="width: 400px;"><input class="form-control" type="text" name="remark" ng-model="item.remark" ng-change="event.remarkText($event,item)"></td>
                                                <td style="width: 100px;">
                                                    @Html.Partial("~/Views/Shared/_Controls/_FileUploadBox2.cshtml", new Frontend.Models.FileUploadViewModel2() { ControlID = "item.labelingPackagingRemarkID", TypeID = 0, URLBinding = "item.remarkFileUrl", FileName = "item.remarkFriendlyName", JSOnChangeHandler = "changeRemarkFile", JSONRemoveHandler = "removeRemarkFile" })
                                                </td>
                                                <td style="width: 100px;">{{item.updatorName}}</td>
                                                <td>{{item.updatedDate}}</td>
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">
                    OK
                </button>
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    Cancel
                </button>
            </div>
        </div>
    </div>
</div>
