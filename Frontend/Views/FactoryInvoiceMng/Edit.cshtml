@{
    if (ViewBag.ID == 0)
    {
        ViewBag.Title = "Create New Factory Invoice";
    }
    else
    {
        ViewBag.Title = "Edit Factory Invoice";
    }

    int bookingId = 0;
    int supplierId = 0;
    string season = "";
    if (Request.Params["b"] != null)
    {
        bookingId = Convert.ToInt32(Request.Params["b"]);
    }
    if (Request.Params["s"] != null)
    {
        supplierId = Convert.ToInt32(Request.Params["s"]);
    }
    if (Request.Params["se"] != null)
    {
        season = Request.Params["se"].ToString();
    }
}
<style>
    .custom-pagination {
        display: none !important;
    }
</style>

<form class="row" name="editForm" id="main-form">
    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
            <header>
                <span class="widget-icon">
                    <i class="fa fa-database"></i>
                </span>
                <h2>General Information</h2>
            </header>
            <div>
                <div class="widget-body no-padding">
                    <div class="smart-form">
                        <fieldset>
                            <div class="row">
                                <section class="col col-2">
                                    <label class="label">Invoice No</label>
                                    <label class="input" ng-if="!data.isConfirmed">
                                        <input type="text" ng-model="data.invoiceNo" ng-if="!data.isConfirmed" />
                                    </label>
                                    <label class="input" ng-if="data.isConfirmed">
                                        <span class="form-control">{{data.invoiceNo}}</span>
                                    </label>
                                </section>
                                <section class="col col-2">
                                    <label class="label">Invoice Date</label>
                                    <label class="input" ng-if="!data.isConfirmed">
                                        <input type="text" class="datepicker" ng-model="data.invoiceDate" data-dateformat="dd/mm/yy" />
                                        <i class="icon-append fa fa-calendar"></i>
                                    </label>
                                    <label class="input" ng-if="data.isConfirmed">
                                        <span class="form-control">{{data.invoiceDate}}</span>
                                    </label>
                                </section>
                                <section class="col col-2">
                                    <label class="label">Season</label>
                                    <label class="input">
                                        <span class="form-control">{{data.season}}</span>
                                    </label>
                                </section>
                                <section class="col col-3">
                                    <label class="label">Booking No</label>
                                    <label class="input">
                                        <span class="form-control">{{data.bookingUD}}</span>
                                    </label>
                                </section>
                                <section class="col col-3">
                                    <label class="label">B/L</label>
                                    <label class="input">
                                        <span class="form-control">{{data.blNo}}</span>
                                    </label>
                                </section>
                            </div>
                            <section>
                                <label class="label">Supplier</label>
                                <label class="input">
                                    <span class="form-control">
                                        <strong>{{data.supplierUD}}</strong> - {{data.supplierNM}}
                                    </span>
                                </label>
                            </section>
                            <div class="row">  
                                <div class="col col-8">
                                    <section class="no-padding">
                                        <label class="label">
                                            Attached File
                                        </label>
                                        <label class="input">
                                            <span class="form-control">
                                                {{data.friendlyName}}
                                                <div style="position: absolute; right: 5px; top: 5px;">
                                                    <a href="javascript:void(0);" class="btn btn-primary btn-xs" style="font-size: 11px;" ng-click="event.downloadFile()">Download</a>
                                                    <a href="javascript:void(0);" class="btn btn-primary btn-xs" style="font-size: 11px;" ng-click="event.changeFile()" ng-if="!data.isConfirmed">Change</a>
                                                    <a href="javascript:void(0);" class="btn btn-primary btn-xs" style="font-size: 11px;" ng-click="event.removeFile()" ng-if="!data.isConfirmed">Remove</a>
                                                </div>
                                            </span>
                                        </label>
                                    </section>
                                    <div class="row no-padding">
                                        <section class="col col-4">
                                            <label class="label">Deducted Amount</label>
                                            <label class="input" ng-if="!data.isConfirmed">
                                                <input type="text" ng-model="data.deductedAmount" />
                                            </label>
                                            <label class="input" ng-if="data.isConfirmed">
                                                <span class="form-control">{{data.deductedAmount | currency:'$':2}}</span>
                                            </label>
                                        </section>
                                        <section class="col col-8">
                                            <label class="label">Remark</label>
                                            <label class="input" ng-if="!data.isConfirmed">
                                                <input type="text" ng-model="data.remark" />
                                            </label>
                                            <label class="input" ng-if="data.isConfirmed">
                                                <span class="form-control">{{data.remark}}</span>
                                            </label>
                                        </section>
                                    </div>
                                </div>
                                <section class="col col-4">
                                    <div class="font-md">
                                        <div>
                                            <strong>STATUS :</strong>
                                            <span class="pull-right" ng-if="!data.isConfirmed"> PENDING </span>
                                            <span class="pull-right" ng-if="data.isConfirmed"> CONFIRMED </span>
                                        </div>
                                    </div>
                                    <div class="font-md">
                                        <div>
                                            <strong>&nbsp;</strong>
                                            <span class="pull-right">&nbsp;</span>
                                        </div>
                                    </div>
                                    <div class="font-md">
                                        <div>
                                            <strong>SUB-TOTAL :</strong>
                                            <span class="pull-right"> {{method.getSubTotal() | currency:'$':2}} </span>
                                        </div>
                                    </div>
                                    <div class="font-md">
                                        <div>
                                            <strong>DEDUCTION :</strong>
                                            <span class="pull-right"> {{data.deductedAmount | currency:'$':2}} </span>
                                        </div>
                                    </div>
                                    <div class="font-md">
                                        <div>
                                            <strong>TOTAL :</strong>
                                            <span class="pull-right"> {{method.getTotal() | currency:'$':2}} </span>
                                        </div>
                                    </div>
                                </section>
                            </div>
                            <section>
                                <div class="note" ng-if="data.updatorName"><strong>Updated By</strong>: {{data.updatorName}} on {{data.updatedDate}}</div>
                                <div class="note" ng-if="data.confirmerName"><strong>Confirmed By</strong>: {{data.confirmerName}} on {{data.confirmedDate}}</div>
                            </section>
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>

        <div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
            <header>
                <ul class="nav nav-tabs pull-left in">
                    <li class="active" ng-if="data.bookingID">
