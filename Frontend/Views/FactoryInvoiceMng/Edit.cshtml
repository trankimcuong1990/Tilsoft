                                                    <div style="width: 80px;">&nbsp;</div>
                                                    <div style="width: 300px;">Description</div>
                                                    <div style="width: 80px;">Qnt</div>
                                                    <div style="width: 80px;">U.Price</div>
                                                    <div style="width: 120px;">Sub-Total</div>
                                                    <div>Remark</div>
                                                </div>
                                                <div class="tilsoft-table-body" style="width: 1000px;">
                                                    <table>
                                                        <tr ng-repeat="item in data.factoryInvoiceDetails">
                                                            <td style="width: 80px; text-align: center;">
                                                                <a ng-click="event.removeProduct(item)" class="btn btn-danger btn-xs font-11 {{data.isConfirmed==true?'disabled' : ''}}" href="javascript:void(0);"><i class=" fa fa-times"></i> Remove</a>
                                                            </td>
                                                            <td style="width: 300px;">
                                                                <strong>Order: {{item.factoryOrderUD}} (Client: {{item.clientUD}})</strong>
                                                                <br />
                                                                <strong>{{item.articleCode}}</strong>
                                                                <br />
                                                                {{item.description}}
                                                            </td>

                                                            <td style="width: 80px; text-align: right;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.quantity" style="width: 100%; text-align: right;" /></td>
                                                            <td style="width: 80px; text-align: right;" ng-if="data.isConfirmed">{{item.quantity | currency:'':0}}</td>

                                                            <td style="width: 80px; text-align: right;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.unitPrice" style="width: 100%; text-align: right;" /></td>
                                                            <td style="width: 80px; text-align: right;" ng-if="data.isConfirmed">{{item.unitPrice | currency:'$':2}}</td>

                                                            <td style="width: 120px; text-align:right">{{$parent.method.getProductSubTotal(item) | currency:'$':2}}</td>

                                                            <td ng-if="!data.isConfirmed"><input type="text" ng-model="item.remark" style="width: 100%;" /></td>
                                                            <td ng-if="data.isConfirmed">{{item.remark}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td style="width: 80px; text-align:right">&nbsp;</td>
                                                            <td style="text-align: right; width: 300px; font-weight: bold;">TOTAL</td>
                                                            <td style="width: 80px; text-align:right; font-weight: bold;">{{method.getProductSumQnt() | currency:'':0}}</td>
                                                            <td style="width: 80px; text-align:right">&nbsp;</td>
                                                            <td style="width: 120px; text-align:right; font-weight: bold;">{{method.getProductSumSubTotal() | currency:'$':2}}</td>
                                                            <td>&nbsp;</td>
                                                        </tr>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </fieldset>
                            </div>
                        </div>
                        <div class="tab-pane" id="tabSparepart" ng-if="data.bookingID">
                            <div class="smart-form">
                                <fieldset>
                                    <div class="row" ng-if="!data.isConfirmed">
                                        <section class="col col-10 furnindo-search-box">
                                            <label class="select">
                                                <select ui-select2 ng-model="selectedFactoryOrderSparepartDetailID" style="width:100%" class="select2" id="factoryOrderSparepartDetailSelector">
                                                    <option value="">-- SELECT --</option>
                                                    <option ng-repeat="item in factoryOrderSparepartDetails | orderBy: 'description'" value="{{item.factoryOrderSparepartDetailID}}">{{item.itemName}}</option>
                                                </select>
                                            </label>
                                        </section>
                                        <section class="col col-2 furnindo-search-button">
                                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="event.addSparepart()"><i class="fa fa-plus"></i> Add</a>
                                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="event.addAllSparepart()"><i class="fa fa-plus"></i> Add all</a>
                                        </section>
                                    </div>
                                    <section>
                                        <div id="grdSparepart" class="tilsoft-table-wrapper">
                                            <div class="tilsoft-table">
                                                <div class="tilsoft-table-header" style="width: 1000px;">
                                                    <div style="width: 80px;">&nbsp;</div>
                                                    <div style="width: 300px;">Description</div>
                                                    <div style="width: 80px;">Qnt</div>
                                                    <div style="width: 80px;">U.Price</div>
                                                    <div style="width: 120px;">Sub-Total</div>
                                                    <div>Remark</div>
                                                </div>
                                                <div class="tilsoft-table-body" style="width: 1000px;">
                                                    <table>
                                                        <tr ng-repeat="item in data.factoryInvoiceSparepartDetails">
                                                            <td style="width: 80px; text-align: center;">
                                                                <a ng-click="event.removeProduct(item)" class="btn btn-danger btn-xs font-11 {{data.isConfirmed==true?'disabled' : ''}}" href="javascript:void(0);"><i class=" fa fa-times"></i> Remove</a>
                                                            </td>
                                                            <td style="width: 300px;">
                                                                <strong>Order: {{item.factoryOrderUD}} (Client: {{item.clientUD}})</strong>
                                                                <br />
                                                                <strong>{{item.articleCode}}</strong>
                                                                <br />
                                                                {{item.description}}
                                                            </td>
                                                            <td style="width: 80px; text-align: right;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.quantity" style="width: 100%; text-align: right;" /></td>
                                                            <td style="width: 80px; text-align: right;" ng-if="data.isConfirmed">{{item.quantity | currency:'':0}}</td>

                                                            <td style="width: 80px; text-align: right;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.unitPrice" style="width: 100%; text-align: right;" /></td>
                                                            <td style="width: 80px; text-align: right;" ng-if="data.isConfirmed">{{item.unitPrice | currency:'$':2}}</td>

                                                            <td style="width: 120px; text-align:right">{{$parent.method.getSparepartSubTotal(item) | currency:'$':2}}</td>

                                                            <td ng-if="!data.isConfirmed"><input type="text" ng-model="item.remark" style="width: 100%;" /></td>
                                                            <td ng-if="data.isConfirmed">{{item.remark}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td style="width: 80px; text-align:right">&nbsp;</td>
                                                            <td style="text-align: right; width: 300px; font-weight: bold;">TOTAL</td>
                                                            <td style="width: 80px; text-align:right; font-weight: bold;">{{method.getSparepartSumQnt() | currency:'':0}}</td>
                                                            <td style="width: 80px; text-align:right">&nbsp;</td>
                                                            <td style="width: 120px; text-align:right; font-weight: bold;">{{method.getSparepartSumSubTotal() | currency:'$':2}}</td>
                                                            <td>&nbsp;</td>
                                                        </tr>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </fieldset>
                            </div>
                        </div>
                        <div class="tab-pane {{data.bookingID == null ? 'active' : ''}}" id="tabExtra">
                            <div class="smart-form">
                                <fieldset>
                                    <section ng-if="!data.isConfirmed">
                                        <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="event.addExtra()"><i class="fa fa-plus"></i> Add</a>
                                    </section>
                                    <section>
                                        <div id="grdExtra" class="tilsoft-table-wrapper">
                                            <div class="tilsoft-table">
                                                <div class="tilsoft-table-header" style="width: 1000px;">
                                                    <div style="width: 80px;">&nbsp;</div>
                                                    <div style="width: 300px;">Description</div>
                                                    <div style="width: 80px;">Qnt</div>
                                                    <div style="width: 80px;">U.Price</div>
                                                    <div style="width: 120px;">Sub-Total</div>
                                                    <div>Remark</div>
                                                </div>
                                                <div class="tilsoft-table-body" style="width: 1000px;">
                                                    <table>
                                                        <tr ng-repeat="item in data.factoryInvoiceExtras">
                                                            <td style="width: 80px; text-align: center;">
                                                                <a ng-click="event.removeExtra(item)" class="btn btn-danger btn-xs font-11 {{data.isConfirmed==true?'disabled' : ''}}" href="javascript:void(0);"><i class=" fa fa-times"></i> Remove</a>
                                                            </td>

                                                            <td style="width: 300px;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.description" style="width: 100%;" /></td>
                                                            <td style="width: 300px;" ng-if="data.isConfirmed">{{item.description}}</td>

                                                            <td style="width: 80px; text-align: right;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.quantity" style="width: 100%; text-align: right;" /></td>
                                                            <td style="width: 80px; text-align: right;" ng-if="data.isConfirmed">{{item.quantity | currency:'':0}}</td>

                                                            <td style="width: 80px; text-align: right;" ng-if="!data.isConfirmed"><input type="text" ng-model="item.unitPrice" style="width: 100%; text-align: right;" /></td>
                                                            <td style="width: 80px; text-align: right;" ng-if="data.isConfirmed">{{item.unitPrice | currency:'$':2}}</td>

                                                            <td style="width: 120px; text-align:right">{{$parent.method.getExtraSubTotal(item) | currency:'$':2}}</td>

                                                            <td ng-if="!data.isConfirmed"><input type="text" ng-model="item.remark" style="width: 100%;" /></td>
                                                            <td ng-if="data.isConfirmed">{{item.remark}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td style="width: 80px; text-align:right">&nbsp;</td>
                                                            <td style="text-align: right; width: 300px; font-weight: bold;">TOTAL</td>
                                                            <td style="width: 80px; text-align:right; font-weight: bold;">{{method.getExtraSumQnt() | currency:'':0}}</td>
                                                            <td style="width: 80px; text-align:right">&nbsp;</td>
                                                            <td style="width: 120px; text-align:right; font-weight: bold;">{{method.getExtraSumSubTotal() | currency:'$':2}}</td>
                                                            <td>&nbsp;</td>
                                                        </tr>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </article>
</form>

<div id="productDetail" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="myModalLabel">Product: {{product.product.description}}</h4>
            </div>
            <div class="modal-body no-padding">
                <div class="smart-form">
                    <fieldset>
                        <div class="row">
                            <section class="col col-3">
                                <label class="label">Factory</label>
                                <label class="input">
                                    <span class="form-control">{{product.product.factoryUD}}</span>
                                </label>
                            </section>

                            <section class="col col-3">
                                <label class="label">Client</label>
                                <label class="input">
                                    <span class="form-control">{{product.product.clientUD}}</span>
                                </label>
                            </section>
                            <section class="col col-3">
                                <label class="label">Factory Order</label>
                                <label class="input">
                                    <span class="form-control">{{product.product.factoryOrderUD}}</span>
                                </label>
                            </section>
                            <section class="col col-3">
                                <label class="label">LDS</label>
                                <label class="input">
                                    <span class="form-control">{{product.product.lds}}</span>
                                </label>
                            </section>
                        </div>
                        <section>
                            <label class="label">Art.Code</label>
                            <label class="input">
                                <span class="form-control">{{product.product.articleCode}}</span>
                            </label>
                        </section>
                        <section>
                            <label class="label">Description</label>
                            <label class="input">
                                <span class="form-control">{{product.product.description}}</span>
                            </label>
                        </section>
                        <div class="row">
                            <section class="col col-3">
                                <label class="label">Ordered qnt</label>
                                <label class="input">
                                    <span class="form-control" style="text-align: right;">{{product.product.orderQnt | currency:'':0}}</span>
                                </label>
                            </section>
                            <section class="col col-3">
                                <label class="label">Load ability (40HC)</label>
                                <label class="input">
                                    <span class="form-control" style="text-align: right;">{{product.product.qnt40HC | currency:'':0}}</span>
                                </label>
                            </section>
                        </div>
                    </fieldset>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" ng-click="event.addProduct()">
                    Add
                </button>
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</div>

<div id="sparepartDetail" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
