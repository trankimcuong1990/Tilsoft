                                                    <option ng-repeat="item in factoryOrderDetails | orderBy: 'description'" value="{{item.factoryOrderDetailID}}">{{item.itemName}}</option>
                                                </select>
                                            </label>
                                        </section>
                                        <section class="col col-2 furnindo-search-button">
                                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="event.addProduct()"><i class="fa fa-plus"></i> Add</a>
                                            <a href="javascript:void(0)" class="btn btn-primary btn-sm" ng-click="event.addAllProduct()"><i class="fa fa-plus"></i> Add all</a>
                                        </section>
                                    </div>

                                    <section>
                                        <div id="grdProduct" class="tilsoft-table-wrapper">
