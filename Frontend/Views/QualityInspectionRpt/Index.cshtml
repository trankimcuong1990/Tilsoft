</form>

<form id="frmCorrectActionSetting" aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" class="modal fade" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <a href="javascript:void(0)" aria-hidden="true" data-dismiss="modal" class="close" ng-click="event.closeCorrectAction()">x</a>
                <h5 id="myModalLabel" class="modal-title">
                    <i class="fa fa-list-ul"></i> Quality Inspection Correct Action
                </h5>
            </div>
            <div class="modal-body">
                <div class="row">
                    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="jarviswidget" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
                            <header>
                                <span class="widget-icon">
                                    <i class="fa fa-database"></i>
                                </span>
                                <h2>General Quality Inspection Correct Action Setting</h2>
                            </header>
                            <div>
                                <div class="jarviswidget-editbox"></div>
                                <div class="widget-body">
                                    <div class="smart-form">
                                        <div class="row">
                                            <section class="col col-sm-3">
                                                <label class="label">Correct Action Code</label>
                                                <label class="input">
                                                    <input type="text" class="form-control" ng-model="editQICorrectActionData.qualityInspectionCorrectActionUD" ng-readonly="editQICorrectActionData.qualityInspectionCorrectActionID > 0">
                                                </label>
                                            </section>
                                            <section class="col col-sm-9">
                                                <label class="label">Correct Action Name</label>
                                                <label class="input">
                                                    <input type="text" class="form-control" ng-model="editQICorrectActionData.qualityInspectionCorrectActionNM">
                                                </label>
                                            </section>
                                        </div>
                                        <div class="row">
                                            <section class="col col-sm-12" style="text-align: right">
                                                <a href="javascript:void(0)" class="btn btn-default btn-sm" ng-click="event.clearCorrectAction()">
                                                    <i class="fa fa-ban"></i> Clear
                                                </a>
                                            </section>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </article>
                    <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="jarviswidget jarviswidget-color" data-widget-editbutton="false" data-widget-colorbutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false">
                            <header>
                                <span class="widget-icon">
                                    <i class="fa fa-search"></i>
                                </span>
                                <h2>Search Quality Inspection Correct Action Setting</h2>
                            </header>
                            <div>
                                <div class="widget-body">
                                    <div class="tilsoft-table-wrapper">
                                        <div class="tilsoft-table">
                                            <div class="tilsoft-table-body" style="margin-top: 0px">
                                                <table>
                                                    <thead>
                                                        <tr>
                                                            <th style="width: 80px">Total<br><b>{{searchQICorrectActionData.length}}</b></th>
                                                            <th style="width: 150px">Code</th>
                                                            <th>Name</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="item in searchQICorrectActionData">
                                                            <td style="text-align: center">
                                                                <a href="javascript:void(0)" class="btn btn-danger btn-xs font-11" style="width: 70px; margin-top: 3px" ng-click="event.deleteCorrectAction(item)">
                                                                    <i class="fa fa-times"></i> Delete
                                                                </a>
                                                            </td>
                                                            <td>
                                                                <span class="form-control">
                                                                    <a href="javascript:void(0)" ng-click="event.editCorrectAction(item)">
                                                                        {{item.qualityInspectionCorrectActionUD}}
                                                                    </a>
                                                                </span>
                                                            </td>
                                                            <td>
                                                                <span class="form-control">{{item.qualityInspectionCorrectActionNM}}</span>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </article>
                </div>
                <div class="row">
                    <section class="col col-sm-12" style="text-align: right">
                        <a href="javascript:void(0)" class="btn btn-default btn-sm" ng-click="event.updateCorrectAction()">
                            <i class="fa fa fa-floppy-o"></i> Save
                        </a>
                        <a href="javascript:void(0)" class="btn btn-default btn-sm" ng-click="event.closeCorrectAction()">
                            <i class="fa fa fa-times"></i> Close
                        </a>
                    </section>
                </div>
            </div>
        </div>
    </div>
</form>

