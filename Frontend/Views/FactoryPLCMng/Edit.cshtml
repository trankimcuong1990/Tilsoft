@{
    if (ViewBag.ID == 0)
    {
        ViewBag.Title = "Create New PLC";
    }
    else
    {
        ViewBag.Title = "Edit PLC";
    }

    int bookingId = 0;
    int factoryId = 0;
    int offerLineId = 0;
    int offerLineSparepartId = 0;
    int offerLineSampleProductId = 0;
    if (Request.Params["b"] != null)
    {
        bookingId = Convert.ToInt32(Request.Params["b"]);
    }
    if (Request.Params["f"] != null)
    {
        factoryId = Convert.ToInt32(Request.Params["f"]);
    }
    if (Request.Params["o"] != null && Request.Params["o"] != "")
    {
        offerLineId = Convert.ToInt32(Request.Params["o"]);
    }
    if (Request.Params["ofs"] != null && Request.Params["ofs"] != "")
    {
        offerLineSampleProductId = Convert.ToInt32(Request.Params["ofs"]);
    }
    if (Request.Params["os"] != null)
    {
        offerLineSparepartId = Convert.ToInt32(Request.Params["os"]);
    }

}

