                                            <div class="furnindo-gallery-item-2" ng-repeat="item in data.plcImages | filter: {imageTypeID: 12} : true">
                                                <a href="{{item.fileLocation}}" data data-featherlight="image"><img ng-src="{{item.thumbnailLocation}}" /></a>
                                                <a class="btn btn-primary btn-xs font-11 gallery-button" href="javascript:void(0)" title="Edit" style="width: 63px; text-align: left;" ng-click="event.openMoveForm(item)"><i class="fa fa-pencil"></i> Move</a>
                                                <a class="btn btn-danger btn-xs font-11 gallery-button" href="javascript:void(0)" title="Remove" style="top: 35px; width: 63px; text-align: left;" ng-click="event.removeImage(item)"><i class="fa fa-times"></i> Remove</a>
                                            </div>
                                            <div class="furnindo-gallery-item-2">
                                                <a href="#" ng-click="event.addImage(12)" title="Add new image"><img src="~/img/add-img-button.jpg" /></a>
                                            </div>
                                            <div class="clear"></div>
                                        </div>
                                    </section>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </article>
</form>

<div id="moveForm" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="myModalLabel">Move the selected image to new group</h4>
            </div>
            <div class="modal-body no-padding">
                <div class="smart-form">
                    <fieldset>
                        <section class="col col-6">
                            <label class="label">Selected image</label>
                            <a href="{{currentImage.fileLocation}}" data data-featherlight="image">
                                <img ng-src="{{currentImage.thumbnailLocation}}" />
                            </a>
                        </section>
                        <section class="col col-6">
                            <label class="label">Select image group to move the image to</label>
                            <div>
                                <label class="radio" ng-repeat="item in plcImageTypes">
                                    <input name="radio-inline" type="radio" ng-value="{{item.plcImageTypeID}}" ng-model="currentImage.imageTypeID">
                                    <i></i>{{item.plcImageTypeNM}}
                                </label>
                            </div>
                        </section>
                    </fieldset>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" ng-click="event.moveFormOK()">
                    OK
                </button>
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    Cancel
                </button>
            </div>
        </div>
    </div>
</div>

