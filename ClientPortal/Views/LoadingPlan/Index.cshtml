@{
    ViewBag.Title = "Search loading report";
    string[] mRibbon = { "Loading report" };
    ViewBag.Ribbon = mRibbon;
    string ControllerName = ViewContext.RouteData.Values["controller"].ToString();
    string ActionName = ViewContext.RouteData.Values["action"].ToString();
}

@Html.Partial("~/Views/LoadingPlan/Index/SearchFilter.cshtml")
@Html.Partial("~/Views/LoadingPlan/Index/SearchResult.cshtml")

@section PageJs {
    <script src="~/Views/@ControllerName/@(ActionName.ToLower()).js"></script>
    <script src="~/Views/@ControllerName/@ActionName/search-filter.js"></script>
    <script src="~/Views/@ControllerName/@ActionName/search-result.js"></script>
    <script src="~/Views/@ControllerName/service.js"></script>
}